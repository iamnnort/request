{
  "name": "@iamnnort/request",
  "version": "1.10.2",
  "description": "Request handler for Node.js - Fast - Interactive - Simple",
  "keywords": [
    "request",
    "fetch",
    "xhr",
    "http",
    "ajax",
    "promise"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "ts-node ./examples/simple",
    "build": "tsup",
    "format": "prettier --write '**/*.{ts,js,json}'",
    "lint": "eslint --fix",
    "test": "yarn lint",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamnnort/request.git"
  },
  "author": "Nikita Pavets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamnnort/request/issues"
  },
  "homepage": "https://github.com/iamnnort/request#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.7.5",
    "@types/qs": "^6.9.16",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1"
  }
}
